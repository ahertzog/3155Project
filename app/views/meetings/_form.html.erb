<%= form_for([@club, @club.meetings.build]) do |f| %>
    
  
    <p>
        Purpose<br>
        <%= f.text_field :purpose %>
    </p>
        
    <p>
        Time and Date<br>
        <%= f.text_area :timeStamp %>
    </p>
        
    <p>
        Location<br>
        <%= f.text_area :location %>
    </p>
        
    <p>
        <%= f.submit %>
    </p>
<% end %>